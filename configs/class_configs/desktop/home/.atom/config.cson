"*":
  "autocomplete-clang":
    clangCommand: "clang-3.5"
  "clang-format":
    formatCOnSave: false
    formatCPlusPlusOnSave: false
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "autocomplete-snippets"
      "linter-cpplint"
      "autocomplete-atom-api"
      "minimap"
      "linter-shellcheck"
      "activate-power-mode"
      "clang-format"
      "autocomplete-clang"
      "linter-clang"
      "language-markdown"
      "nuclide"
      "remote-edit"
    ]
    excludeVcsIgnoredPaths: false
    telemetryConsent: "no"
    warnOnLargeFileLimit: 10
  editor:
    showInvisibles: true
    softWrap: true
    tabLength: 4
  "exception-reporting":
    userId: "d3d9a1ba-9434-12d6-c4ba-691a7b4b9924"
  "linter-clang": {}
  "linter-pylint":
    executablePath: "/usr/bin/clang++-3.5"
  "linter-shellcheck":
    shellcheckExecutablePath: "/home/ian/.cabal/bin/shellcheck"
  "linter-ui-default": {}
  minimap:
    displayMinimapOnLeft: true
  "remote-edit":
    storePasswordsUsingKeytar: true
  "spell-check":
    useLocales: false
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace: {}
".bat.cmake.source.template":
  editor:
    tabLength: 4
".c.cmake.source.template":
  editor:
